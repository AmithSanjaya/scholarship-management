
@{
    ViewBag.Title = "SendProgressNotification";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <div class="row">
        @{ Html.RenderAction("StudentView", "Form");}
        @{ Html.RenderAction("CommonView", "Form");}
        <div class="col-sm-12">
            <div class="card">
                <div class="card-header d-flex justify-content-between">
                    <div class="header-title">
                        <h4 class="card-title">Send Student's Progress Notification</h4>
                    </div>
                    @{ Html.RenderAction("FormButton", "Admin");}
                </div>
                <div class="card-body">
                    <form action="page-list-customers.html" data-toggle="validator">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Effective Year :</label>
                                    <select id="EffectiveYear" class="selectpicker form-control" data-style="py-0"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="col-lg-12">
                    <div class="table-responsive rounded mb-3">
                        <table class="data-tables table mb-0 tbl-server-info" id="datable_1">
                            <thead class="bg-white text-uppercase">
                                <tr class="ligth ligth-data">
                                    <th hidden>StudentID</th>
                                    <th>Student Name</th>
                                    <th>Gender</th>
                                    <th>District</th>
                                    <th>School Name</th>
                                    <th>Send Progress</th>
                                    <th>Send Notifications</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody class="ligth-body" id="FillProgresNotification">

                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="col-lg-12">
                    <hr />
                    <p>Students to be send Progress Notification</p>
                    <hr />

                    <div class="table-responsive rounded mb-3">
                        <table class="data-table table mb-0 tbl-server-info" id="datable_1">
                            <thead class="bg-white text-uppercase">
                                <tr class="ligth ligth-data">
                                    <th hidden>StudentID</th>
                                    <th>Student Name</th>
                                    <th>Gender</th>
                                    <th>District</th>
                                    <th>School Name</th>
                                    <th hidden>ProgressSend</th>
                                    <th hidden>ProgressNotification</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody class="ligth-body" id="ProgresNotification">
                                
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- Page end  -->
        </div>
    </div>
</div>

<script async src="~/Scripts/PageJs/SendProgressNotification.js"></script>


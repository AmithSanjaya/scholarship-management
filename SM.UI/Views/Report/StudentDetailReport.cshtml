
@{
    ViewBag.Title = "StudentDetailReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <div class="row">

        <div class="col-sm-12">
            <div class="card">
                <div class="card-header d-flex justify-content-between">
                    <div class="header-title">
                        <h4 class="card-title">Student Detail Report</h4>
                    </div>
                    <div class="add-list">
                        <button type="button" class="btn btn-secondary mt-2" onclick="Preview();">Preview</button>
                    </div>
                </div>
                <div class="card-body">
                    <form action="page-list-customers.html" data-toggle="validator">

                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Student Country :</label>
                                    <select id="StudentCounty" class="selectpicker form-control" data-style="py-0"></select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Student District :</label>
                                    <select id="StudentDistrict" class="selectpicker form-control" data-style="py-0"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <!-- Page end  -->
        </div>
    </div>
</div>

<script async src="~/Scripts/PageJs/StudentDetailReport.js"></script>


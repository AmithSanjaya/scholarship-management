<div class="modal fade student-modal" tabindex="-1" role="dialog" aria-hidden="true" id="sponsermodel">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Preview Sponser</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <input type="hidden" id="ModelSponserID" />
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card car-transparent">
                            <div class="card-body p-0">
                                <div class="profile-image position-relative">
                                    <img src="../assets/images/page-img/profile.png" class="img-fluid rounded w-100" alt="profile-image">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row m-sm-0 px-3">
                    <div class="col-lg-4 card-profile">
                        <div class="card card-block card-stretch card-height">
                            <div class="card-body">
                                <div class="d-flex align-items-center mb-3">
                                    <div class="profile-img position-relative">
                                        <img src="../assets/images/user/1.png" class="img-fluid rounded avatar-110 StudentImg" alt="profile-image" id="StudentImg">
                                    </div>
                                    <div class="ml-3">
                                        <h4 class="mb-1" id="#SponserName"></h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-8 card-profile">
                        <div class="card card-block card-stretch card-height">
                            <div class="card-body">
                                <ul class="d-flex nav nav-pills mb-3 text-center profile-tab" id="profile-pills-tab" role="tablist">
                                    <li class="nav-item">
                                        <a class="nav-link active show" data-toggle="pill" href="#PersonalInformation" role="tab" aria-selected="false">Profile Information</a>
                                    </li>
                                </ul>
                                <div class="profile-content tab-content">

                                    <div id="PersonalInformation" class="tab-pane fade active show">

                                        <div class="row">
                                            <div class="table-responsive">
                                                <table class="table table-striped jambo_table bulk_action" id="tblDetails">

                                                    <tr>
                                                        <td>Contact No</td>
                                                        <td class="SponcerContactNo"></td>
                                                    </tr>
                                                    <tr>
                                                        <td>Email</td>
                                                        <td class="SponcerEmail"></td>
                                                    </tr>
                                                    <tr>
                                                        <td>Address</td>
                                                        <td class="SponcerAddress"></td>
                                                    </tr>
                                                    <tr>
                                                        <td>Country</td>
                                                        <td class="SponcerCountry"></td>
                                                    </tr>
                                                    <tr>
                                                        <td>Payment Scheme</td>
                                                        <td class="SponcerPayTerm"></td>
                                                    </tr>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

